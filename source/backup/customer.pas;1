  SysUtils, Person;

type

     { TCustomerType }

  TCustomerType = class(TPerson)


  end;

    { TCustomer }

  TCustomer = class(TPerson)
  private
    FType: TCustomerType;
  public

  published
     property Customer: TCustomerType read getCustomerType write setCustomerType;
  end;

implementation


end.
